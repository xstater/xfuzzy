{
  "name": "xfuzzy",
  "displayName": "xfuzzy",
  "description": "fuzzy finder in vscode",
  "publisher": "xstater",
  "author": {
    "name": "xstater",
    "email": "a166224@163.com",
    "url": "https://github.com/xstater"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xstater/xfuzzy.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "keywords": [
    "fuzzy",
    "finder",
    "ripgrep"
  ],
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "xfuzzy.searchInWorkspace",
        "title": "XFuzzy: Search in workspace"
      },
      {
        "command": "xfuzzy.searchInActiveEditor",
        "title": "XFuzzy: Search in active editor"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@vscode/ripgrep": "^1.15.3",
    "ripgrep-wrapper": "^1.1.1"
  }
}